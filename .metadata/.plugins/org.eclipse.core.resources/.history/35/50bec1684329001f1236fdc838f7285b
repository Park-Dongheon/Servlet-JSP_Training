<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>구구단</title>
</head>
<body>
	<%
	int col = 2;

	try {
		col = Integer.parseInt(request.getParameter("col"));

	} catch (Exception e) {}
	
	
	int dan = 2;
	
	while (dan <= 9) {
		for (int i = 0; i < col && dan <= 9; i++, dan++) {
			out.print("<h2>" + dan + "단</h2>");
			for (int j = 1; j <= 9; j++) {
				out.print(dan + "*" + j + "=" + (dan * j) + "<br />");				
			}
		}
	}
	%>
</body>
</html>