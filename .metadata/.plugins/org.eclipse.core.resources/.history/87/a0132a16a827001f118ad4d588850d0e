<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%!
public String gugudan(int col) {
	StringBuffer result = new StringBuffer();
	result.append("<br/>");
	
	int k = 3;
	for(int i = 1; i < 10; i++) {
		for(int j = 1; j < 10; j++) {
			if(col < k) {
				result.append(j).append("*").append(i).append("=").append(i * j);
                if(j != 9) {
                    result.append(" ");                   
                }
			}
		}
        result.append("<br/>");
	}

	return result.toString();
}
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<titleng().>구구단</title>
</head>
<body>
<%
String result;

try {
	// http://localhost:8080/missionJSP/Gugudan1.jsp?dan=2
	int col = Integer.parseInt(request.getParameter("col"));
	
	result = gugudan(col);			
} catch (Exception e) {
	result = "Invalid";
}
%>
<%= result %>

</body>
</html>